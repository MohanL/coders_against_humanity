extends layout

block content

	h1.page-title Pick your white card...

	.container
		.row
			.card.card-black Card test ____ with ____ holes.
		.row
			.spacer
		.row
			table.table
				tr
					th.success Team 1
					th.warning Team 2
					th.danger Team 3
					th.success Team 4
			.container-fluid
				.col-xs-2
					a.btn.btn-warning(href="/screen/show_results") End Round
				.col-xs-10
					.progress
						.progress-bar.progress-bar-warning.progress-bar-striped.active(role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="width: 40%")
							span.sr-only 40% Complete
